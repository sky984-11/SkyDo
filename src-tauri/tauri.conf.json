{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "SkyDo",
    "version": "0.5.21"
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/sky984-11/SkyDo/releases/download/updater/update.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDZDRkUyOTg0MUVCMUI3MTUKUldRVnQ3RWVoQ24rYkR3THQyV0dJK1pjdUVDazcwSG05OTlrankrSmVjRityaU1IM3JTN3l3QkcK%"
    },
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      },
      "process": {
        "relaunch": true
      },
      "dialog": {
        "all": true,
        "ask": true, 
        "confirm": true, 
        "message": true,
        "open": true,
        "save": true 
      },
      "fs": {
        "all": true,
        "scope": [
          "$APPDATA/*",
          "$APPDATA/images/*"
        ]
      },
      "notification": {
        "all": true
      },
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "check-tailscale-isExit",
            "cmd": "tailscale",
            "args": [
              "version"
            ]
          },
          {
            "name": "check-tailscale-ping",
            "cmd": "tailscale",
            "args": [
              "ping",
              {
                "validator": ".*?"
              }
            ]
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.TProxy.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "dangerousUseHttpScheme": true,
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    },
    "windows": [
      {
        "fullscreen": false,
        "title": "SkyDo",
        "width": 400,
        "height": 450,
        "minWidth": 300,
        "minHeight": 250,
        "transparent": true,
        "decorations": false,
        "skipTaskbar": true,
        "fileDropEnabled": false,
        "x": 1050,
        "y": 80
      }
    ],
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": false
    }
  }
}